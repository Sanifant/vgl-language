{==============================================================================}
{ Method for class nwa_update_setup                                            }
{ Change the setup root path and other necessary settings related to the       }
{ current setup (e.g. release candidate)                                       }
{==============================================================================}
{ Parameter          Type            Comment                                   }
{------------------------------------------------------------------------------}
{ self               Object          "Pointer to own object"  (defaultparam)   }
{                                    This parameter doesn't appear in the      }
{                                    actual Paramterlist.                      }
{ rootIndex          Integer         The index of the release candidate.       }
{                                    0 = Main                                  }
{==============================================================================}
{ Return-Value: None                                                           }
{=[05/10/2021 (SP)]============================================================}
ROUTINE nwa_update_setup_action_ChangeRootPath(self, VALUE rootIndex)

   {
   self.WIZ_ExplorerCabinetWizardActive  = FALSE
   }

   IF IsValue(rootIndex) THEN
      IF (rootIndex = 0) THEN
         self.setupRootPath = self.mainRootPath
      ELSEIF (rootIndex = 1) THEN
         self.setupRootPath = self.rc01RootPath
      ELSEIF (rootIndex = 2) THEN
         self.setupRootPath = self.rc02RootPath
      ENDIF {<- IF (rootIndex = 1) THEN }
   ELSE
      self.setupRootPath = self.mainRootPath
   ENDIF {<- IF IsValue(rootIndex) THEN }

   self.lsd_setup_update_base::ChangeRootPath(rootIndex)

ENDROUTINE { nwa_update_setup_action_ChangeRootPath }


{==============================================================================}
{ Method for class nwa_update_setup                                            }
{ Could be overwritten in descendant class to update some options add          }
{ additional command lines etc. before running the real update                 }
{ Called after user dialog has been shown with the available options.          }
{==============================================================================}
{ Parameter          Type            Comment                                   }
{------------------------------------------------------------------------------}
{ self               Object          "Pointer to own object"  (defaultparam)   }
{                                    This parameter doesn't appear in the      }
{                                    actual Paramterlist.                      }
{==============================================================================}
{ Return-Value: None                                                           }
{=[05/10/2021 (SP)]============================================================}
ROUTINE nwa_update_setup_action_AfterUpdateActiveOptions(self)

   {DECLARE iCmd                         Index for configuration array         }

   {---------------------------------------------------------------------------}

   self.AddFolderToZipFolderList(self.setupRootPath, "FULL", TRUE)
   {self.AddFolderToZipFolderListV2(self.setupRootPath, "FULL", TRUE, self.emailRecipients  : ";other.user@customer.com")}

   IF (self.CURRENT_ACTIVE_ReleaseCandidate = 1) THEN
      self.AddFolderToZipFolderList(self.rc01RootPath, "RC01", TRUE)
   ELSEIF (self.CURRENT_ACTIVE_ReleaseCandidate = 2) THEN
      self.AddFolderToZipFolderList(self.rc02RootPath, "RC02", TRUE)
   ENDIF {<- IF (self.CURRENT_ACTIVE_ReleaseCandidate = 1) THEN }

   {---------------------------------------------------------------------------}

   { Example for additional command lines (legacy for old versions):
   iCmd = size_of_array(self.additionalCommandLines)

   Inc(iCmd)
   self.additionalCommandLines[iCmd, addCmdLineArr_ColBatchName]   = ChangeLogicalInFileName(self.setupRootPath : "update_full_setup_files.bat")
   self.additionalCommandLines[iCmd, addCmdLineArr_ColLogFileName] = ChangeFileExt(self.additionalCommandLines[iCmd, addCmdLineArr_ColBatchName], ".log")
   self.additionalCommandLines[iCmd, addCmdLineArr_ColAddParam]    = ""
   }

   {---------------------------------------------------------------------------}

ENDROUTINE


