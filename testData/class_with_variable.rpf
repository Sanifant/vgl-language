   {             ids to exclude from the export.                               }
   {    Param 3: If true export cabinets with $ in the nanme (only valid if    }
   {             Param 2 is set to FALSE)                                      }
   { WIZ_ExplorerCabinetPrepareCabinetIDsV2                                    }
   {    Param 1: Comma separated list of cabinet IDs to include.               }
   {             If id contains a % sign the compare is done with LIKE,        }
   {             otherwise with = .                                            }
   {             "" means export all cabinets.                                 }
   {    Param 2: Comma separated list of cabinet IDs to exclude.               }
   {             If id contains a % sign the compare is done with LIKE,        }
   {             otherwise with = .                                            }
   {             (Only necessary if Param 1 contains LIKEs.                    }
   {---------------------------------------------------------------------------}
   self.WIZ_ExplorerCabinetWizardActive               = self.defaultWIZ_ExplorerCabinetWizardActive
   self.WIZ_ExplorerCabinetExpExplorer                = self.defaultWIZ_ExplorerCabinetExpExplorer
   self.WIZ_ExplorerCabinetShowEPW                    = self.defaultWIZ_ExplorerCabinetShowEPW
   self.WIZ_ExplorerCabinetExpMasterMenu              = self.defaultWIZ_ExplorerCabinetExpMasterMenu
   self.WIZ_ExplorerCabinetExpMenuToolbar             = self.defaultWIZ_ExplorerCabinetExpMenuToolbar
   self.WIZ_ExplorerCabinetExpRoleEntry               = self.defaultWIZ_ExplorerCabinetExpRoleEntry
   self.WIZ_ExplorerCabinetRoleEntryRoleIDs           = self.defaultWIZ_ExplorerCabinetRoleEntryRoleIDs
   self.WIZ_ExplorerCabinetExpMenuToolbarSingleFiles  = self.defaultWIZ_ExplorerCabinetExpMenuToolbarSF
   self.WIZ_ExplorerCabinetExpMenuToolbarTBNames      = self.defaultWIZ_ExplorerCabinetExpMenuToolbarTBN
   {array_select_add(self.WIZ_ExplorerCabinetExpMenuToolbarSelArr, ARRAY_SELECT_EQ, "TOOLBAR_NAME", "SYSTEM")}
   {self.WIZ_ExplorerCabinetPrepareCabinetIDs("USER", FALSE, FALSE)}
   {self.WIZ_ExplorerCabinetPrepareCabinetIDs("TABLE_DETAILS,CUSTCABINET1", TRUE, FALSE)}
   {self.WIZ_ExplorerCabinetPrepareCabinetIDsV2("TABLE_DETAILS,CUSTCAB%", "")}


   {-----------------------------------------}
   { Config items (copy paste to setup file) }
   {-----------------------------------------}
   {self.WIZ_CreateStringsForCustConfigItems("")}


   {--------------------------}
   { Menu toolbar - MTB files }
   {--------------------------}
   {self.ExportMenuToolbarTree("SYSTEM:0000")}


   self.lsd_setup_update_base::OtherActions()

ENDROUTINE


